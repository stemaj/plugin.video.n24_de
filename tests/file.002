                            <img height="38px" width="54px" src="${ assetsBaseDir.replace('/images/', weatherStateImage.small).replace('/d_', '/d_w_') }" alt="${ weatherStateText }">
                        <% } else { %>
                            <img height="38px" width="54px" src="${ assetsBaseDir.replace('/images/', weatherStateImage.small) }" alt="${ weatherStateText }">
                        <% } %>

                        <span class="[ epsilon ]">${ temperatureMax }°</span>
                        <span class="[ text--small ]"> / ${ temperatureMin }°</span>
                    </span>
                </a>
            </li>
        </script>
    </ul>

